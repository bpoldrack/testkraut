{
  "id": "check_fsl_workflow", 
  "environment": {
    "FSLDIR": true,
    "FSLOUTPUTTYPE": false
  }, 
  "dependencies": {
    "bet": {
      "location": "$FSLDIR/bin/bet",
      "type": "executable",
      "version_cmd": [
            "$FSLDIR/bin/bet2",
            "BET \\(Brain Extraction Tool\\) v(\\S+) -"
      ]
    }, 
    "bet2": {
      "location": "$FSLDIR/bin/bet2",
      "type": "executable",
      "version_file": "$FSLDIR/etc/fslversion"
    }
  }, 
  "inputs": {
    "head image": {
      "sha1sum": "41d817176ceb99ac051d8bd066b500f3fb89be89", 
      "tags": [
        "3D image", 
        "volumetric image"
      ], 
      "type": "file", 
      "value": "head.nii.gz"
    }
  }, 
  "outputs": {
    "brain image": {
      "tags": [
        "3D image", 
        "nifti1 format", 
        "volumetric image"
      ], 
      "type": "file", 
      "value": "brain.nii.gz"
    }, 
    "brain mask image": {
      "tags": [
        "3D image", 
        "nifti1 format", 
        "volumetric image"
      ], 
      "type": "file", 
      "value": "brain_mask.nii.gz"
    }
  }, 
  "tests": [
    {
      "code": "$FSLDIR/bin/bet head brain -m",
      "type": "shell"
    }
  ],
  "version": 0
}
